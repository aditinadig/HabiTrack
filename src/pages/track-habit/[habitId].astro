---
import TrackingPageComponent from "../habits/TrackingPage.jsx";
import { getAllHabitIds } from "../../services/habitService";
const { habitId } = Astro.params;

export async function getStaticPaths() {
  const habitIds = await getAllHabitIds(); // Same as above, reuse the function for habit IDs
  return habitIds.map((id) => ({ params: { habitId: id } }));
}
---

<TrackingPageComponent habitId={habitId} client:only="react"/>